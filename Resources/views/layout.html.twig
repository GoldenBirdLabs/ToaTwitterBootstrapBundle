{% extends '::base.html.twig' %}


{%- block body -%}
        <div class="container">
            {%- block content '' %}
        </div>
{%- endblock %}


{%- block stylesheets -%}
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {%- block stylesheets_toa_twitter_bootstrap %}

        <link href="{{ asset('bundles/toatwitterbootstrap/twitterbootstrap/docs/assets/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />

    {%- endblock %}

    {%- block stylesheets_toa_twitter_bootstrap_responsive %}

        <link href="{{ asset('bundles/toatwitterbootstrap/twitterbootstrap/docs/assets/css/bootstrap-responsive.css') }}" rel="stylesheet" type="text/css" />

    {%- endblock %}

    {{- parent() }}

{%- endblock -%}


{%- block javascripts %}

        <script src="{{ asset('bundles/toatwitterbootstrap/twitterbootstrap/docs/assets/js/jquery.js') }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/toatwitterbootstrap/twitterbootstrap/docs/assets/js/bootstrap.js') }}" type="text/javascript"></script>
        {{- parent() }}

{%- endblock %}
