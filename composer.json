{
    "name": "toa/twitter-bootstrap-bundle",
    "type": "symfony-bundle",
    "description": "Basic integration of Twitter Bootstrap into Symfony2 [wip]",
    "keywords": ["code generation"],
    "license": "MIT",
    "version": "master",
    "authors": [
        {
            "name": "Enrico Thies",
            "email": "enrico.thies@gmail.com"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/toaotc/ToaTwitterBootstrapBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "2.1.*",
        "sensio/generator-bundle": "dev-master"
    },
    "suggest": {
        "twitter/bootstrap": "2.0.3"
    },
    "repositories": {
        {
            "type": "package",
            "package": {
                "version": "2.0.3",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "vcs",
                    "reference": "v2.0.3"
                }
            }
        }
    },
    "autoload": {
        "psr-0": {
            "Toa\\Bundle\\TwitterBootstrapBundle": ""
        }
    },
    "target-dir": "Toa/Bundle/TwitterBootstrapBundle"
}